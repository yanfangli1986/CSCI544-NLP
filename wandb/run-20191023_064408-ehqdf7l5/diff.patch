+#!/usr/bin/env bash
+
+declare -i idx=0
+
+for emb_dim in 50 100 200 300 500
+do
+    for hid_dim in 100 200 400 600 1000
+    do
+        for dropout_ratio in 0.3 0.4 0.5 0.6 0.7
+        do
+            let idx+=1
+            # echo "($idx) emb_dim = $emb_dim hid_dim = $hid_dim dropout_ratio = $dropout_ratio"
+            nohup sh run_lm_rnn.sh 'LSTM' $emb_dim $hid_dim $dropout_ratio > "./EXP/log_$idx.txt" &
+        done
+    done
+done
+
